    "# Decorators\n",
    "\n",
    "## â€¦ wrap around a function, returning a new function."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def decorator(fun):\n",
    "    print(\"before\")\n",
    "    fun()\n",
    "    print(\"after\")\n",
    "\n",
    "@decorator\n",
    "def do_something():\n",
    "    print(\"doing real work here!\")"
   ]
  }
